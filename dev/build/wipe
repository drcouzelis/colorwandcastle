#!/usr/bin/bash

source "$(dirname ${0})"/settings

\rm -vrf "${working_dir}"

\rm -vrf "${source_dir}"/autom4te.cache
\rm -vf "${source_dir}"/Makefile.in
\rm -vf "${source_dir}"/aclocal.m4
\rm -vf "${source_dir}"/compile
\rm -vf "${source_dir}"/config.h
\rm -vf "${source_dir}"/config.h.in
\rm -vf "${source_dir}"/config.h.in~
\rm -vf "${source_dir}"/configure
\rm -vf "${source_dir}"/depcomp
\rm -vf "${source_dir}"/install-sh
\rm -vf "${source_dir}"/missing
